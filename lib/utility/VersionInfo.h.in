#ifndef QUENTIER_VERSION_INFO_H
#define QUENTIER_VERSION_INFO_H

/**
 * This file contains automatically generated code including macro definitions
 * which determine packaging-specific properties of Quentier
 */

/**
 * This macro is set to true if quentier was packaged as AppImage on Linux
 */
@QUENTIER_PACKAGED_AS_APP_IMAGE_DEFINE@

/**
 * This macro contains the name of libquentier binary found by CMake
 */
@QUENTIER_LIBQUENTIER_BINARY_NAME@

/**
 * This macro contains the name of QEverCloud binary found by CMake
 */
@QUENTIER_QEVERCLOUD_BINARY_NAME@

/**
 * This macro denotes the major version number of Quentier
 */
@QUENTIER_MAJOR_VERSION@

/**
 * This macro denotes the minor version number of Quentier
 */
@QUENTIER_MINOR_VERSION@

/**
 * This macro denotes the patch version number of Quentier
 */
@QUENTIER_PATCH_VERSION@

/**
 * This macro is set to a string holding the information about the current
 * Quentier build
 */
@QUENTIER_BUILD_INFO@

#endif // QUENTIER_VERSION_INFO_H
