#pragma once

#include <lib/preferences/UpdateSettings.h>

/**
 * This file contains automatically generated code including macro definitions
 * which determine version/release specific properties of Quentier used to
 * check for updates
 */

/**
 * This macro contains the name of update channel which Quentier should use
 * for updates checking by default
 */
@QUENTIER_DEFAULT_UPDATE_CHANNEL@

/**
 * This macro contains the name of update provider which Quentier should use
 * for updates checking by default
 */
@QUENTIER_DEFAULT_UPDATE_PROVIDER@

/**
 * This macro contains the timestamp of current Quentier build which Quentier
 * would use to check for updates later
 */
@QUENTIER_BUILD_TIMESTAMP@

/**
 * This macro contains the git branch from which Quentier was built or empty
 * string if this information is not available
 */
@QUENTIER_BUILD_GIT_BRANCH@

/**
 * This macro contains the git commit hash from which Quentier was build or
 * empty string if this information is not available
 */
@QUENTIER_BUILD_GIT_COMMIT@
